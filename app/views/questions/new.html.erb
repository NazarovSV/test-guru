<h1>Create new question:</h1>
<%= render 'shared/errors', resource: @question%>

<%= form_with model: [@test, @question], local: true do |form| %>
  <p>
    <%= form.label :body, "Question:" %>
    <%= form.text_field :body, id: :question_body %>
  </p>
  <%= form.submit "Create question" %>
<% end %>
<br/>
<%= link_to 'Back', test_path(@test) %>