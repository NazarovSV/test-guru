<!DOCTYPE html>
<html>
  <head>
    <title>Admin Panel | TestGuru</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
      <div class="wrapper">
        <div class="content">
          <%= render 'admin/nav' %>

          <div class="container">
            <%= render 'shared/flash' %>
            <%= yield %>
          </div >
        </div >
        <%= render 'shared/footer' %>
    </div>
  </body>
</html>
